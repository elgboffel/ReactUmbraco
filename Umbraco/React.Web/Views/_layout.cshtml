@using React.Web.Mvc
@using React.Web.Models.React
@using ClientDependency.Core.Mvc
@inherits UmbracoViewPage
@{
    Layout = null;

    Html.RequiresJs("https://cdnjs.cloudflare.com/ajax/libs/react/16.2.0/umd/react.production.min.js", 1, 3, new { @defer = string.Empty });
    Html.RequiresJs("https://cdnjs.cloudflare.com/ajax/libs/react-dom/16.2.0/umd/react-dom.production.min.js", 1, 3, new { @defer = string.Empty });
    Html.RequiresJs("https://cdnjs.cloudflare.com/ajax/libs/remarkable/1.7.1/remarkable.min.js", 1, 3, new { @defer = string.Empty });
    //Html.RequiresJs("https://cdnjs.cloudflare.com/ajax/libs/react-slick/0.20.0/react-slick.min.js", 1, 3, new { @defer = string.Empty });
    Html.RequiresJs("~/scripts/build/bundles/bundle.js", 1, 3, new { @defer = string.Empty });
}

<!DOCTYPE html>
<html lang="en">
<head>

    <!-- Meta tags -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">

    <title></title>

    <!-- Fonts -->

    <!-- CSS -->

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="//oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="//oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>

<body>
    <div class="view"></div>
    @RenderBody()
    <!-- Javascripts -->
    @Html.RenderJsHere()
</body>
</html>

